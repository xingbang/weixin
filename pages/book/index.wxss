.container{
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 100%;
}
.sub-container{
  display: flex;
  flex-direction: column;
  align-items: center;
  background: #f5f5f5;
  margin-top: 100rpx;
}
.box{
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  border-radius: 50rpx;
  background-color: #f5f5f5;
  width: 700rpx;
  height: 68rpx;
  color: #999;
}
.box image{
  margin-right:10px;
  width:14px;
  height:14px;
  margin-bottom:-2px;
}
.header{
  position:fixed;
  background:#fff;
  height: 100rpx;
  width: 100%;
  border-top: 1px solid #f5f5f5;
  border-bottom: 1px solid #f5f5f5;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  box-shadow: 0 0 3px 0 #e3e3e3;
  z-index: 99;
}
.books-container{
  margin-top: 10rpx;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: space-between;
  padding: 0 90rpx;
}
.head-img{
  width:106rpx;
  height:34rpx;
  margin-top:40rpx;
}
.books-container v-book{
  margin-bottom: 10rpx;
}